{
  "name": "method-social",
  "version": "1.0.0",
  "description": "Method Social Media Using Typescript and Type ORM",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "build": "rm -rf dist && tsc",
    "postinstall": "npm run build",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:create-migration": "npm run typeorm -- migration:create -n",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./src/utils/database/datasource.ts",
    "typeorm:revert-migrations": "npm run typeorm migration:revert -- -d ./src/utils/database/datasource.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hadybabaei/leading-typescript.git"
  },
  "keywords": [
    "method"
  ],
  "author": "Hadybabaei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hadybabaei/leading-typescript/issues"
  },
  "homepage": "https://github.com/Hadybabaei/leading-typescript#readme",
  "devDependencies": {
    "@prisma/client": "^5.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-facebook": "^3.0.2",
    "@types/pg": "^8.10.2",
    "eslint": "^8.48.0",
    "eslint-plugin-typescript": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.0.4",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/express-session": "^1.17.9",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.11",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
